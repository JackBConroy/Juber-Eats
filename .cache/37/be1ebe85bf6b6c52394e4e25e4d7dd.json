w-x: scroll;\r\n    scroll-snap-type: x mandatory;\r\n    scroll-behavior: smooth;\r\n\r\n    &::-webkit-scrollbar {\r\n      display: none;\r\n    }\r\n\r\n    .listings-col {\r\n      display: flex;\r\n      flex-direction: row;\r\n      flex-wrap: wrap;\r\n      justify-content: space-between;\r\n      flex-grow: 0;\r\n      flex-shrink: 0;\r\n      flex-basis: 100%;\r\n      scroll-snap-align: start;\r\n      margin-left: 25px;\r\n    }\r\n\r\n    .listings-grid-element {\r\n      width: calc(1 / 3 * 100% - (1 - 1 / 3) * 25px);\r\n      margin-top: 2rem;\r\n\r\n      .image {\r\n        height: 240px;\r\n        overflow: hidden;\r\n        img {\r\n          width: 100%;\r\n          height: 100%;\r\n          object-fit: cover;\r\n        }\r\n      }\r\n      .text-lower {\r\n        padding: 0.6rem 0;\r\n        span {\r\n          color: $color-dark-grey;\r\n        }\r\n      }\r\n      .text {\r\n        display: flex;\r\n        align-items: center;\r\n        border-bottom: 1px solid #ddd;\r\n        padding: 1rem 0;\r\n        h3 {\r\n          font-weight: 400;\r\n          margin-bottom: 0.3rem;\r\n          margin-top: 0;\r\n        }\r\n        span {\r\n          font-size: 0.8rem;\r\n          font-weight: 600;\r\n        }\r\n        .rating {\r\n          margin: auto 0 auto auto;\r\n          height: 100%;\r\n\r\n          span {\r\n            height: 40px;\r\n            width: 40px;\r\n            background: $color-grey;\r\n            font-size: 1rem;\r\n            color: black;\r\n          }\r\n        }\r\n      }\r\n    }\r\n  }\r\n}\r\n","styles\\modules\\_variables.scss":"$color-green: #3fc060;\r\n$font: sans-serif;\r\n$color-grey: #dddd;\r\n$color-dark-grey: rgba(84, 84, 84);\r\n","styles\\modules\\_header.scss":"header {\r\n  background-color: #000;\r\n  color: #fff;\r\n  .logo {\r\n    h1 {\r\n      font-weight: 300;\r\n      span {\r\n        font-weight: 600;\r\n        color: $color-green;\r\n      }\r\n    }\r\n  }\r\n  .container {\r\n    display: flex;\r\n    align-items: center;\r\n  }\r\n  .searchBar {\r\n    margin-left: auto;\r\n    cursor: pointer;\r\n  }\r\n  .header-option {\r\n    padding-left: 5rem;\r\n    letter-spacing: 0.6px;\r\n    display: flex;\r\n    align-items: center;\r\n    svg {\r\n      margin-right: 1rem;\r\n      display: inline-flex;\r\n    }\r\n  }\r\n  .currentDetails,\r\n  .searchBar {\r\n    display: flex;\r\n  }\r\n}\r\n\r\n.options {\r\n  padding: 3.5rem 0 1.5rem 0;\r\n  .options-btn {\r\n    background: $color-grey;\r\n    padding: 0.6rem 0.8rem;\r\n    border-radius: 30px;\r\n    border: 0;\r\n    font-family: $font;\r\n    font-size: 1rem;\r\n    letter-spacing: 0.6px;\r\n    display: inline-flex;\r\n    align-items: center;\r\n    margin-right: 0.3rem;\r\n    cursor: pointer;\r\n    svg {\r\n      max-width: 20px;\r\n      margin-right: 0.4rem;\r\n    }\r\n    &.selected {\r\n      background: black;\r\n      color: white;\r\n    }\r\n  }\r\n}\r\n","styles\\modules\\_global.scss":"* {\r\n  box-sizing: border-box;\r\n}\r\n\r\n.smallText {\r\n  font-size: 0.7rem;\r\n  font-weight: 300;\r\n}\r\n\r\nbody {\r\n  margin: 0;\r\n  font-family: sans-serif;\r\n}\r\n\r\n.container {\r\n  width: calc(100% - 120px);\r\n  margin: auto;\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"f0f0a747682bed392c97ebf1675a1f2b","cacheData":{"env":{}}}